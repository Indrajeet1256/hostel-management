{
    "version": 2,
    "builds": [
        {
              "src": "dist/index.js", 
              "use": "@vercel/node",
              "config": { "includeFiles": ["dist/**"] }
        },
        {
            "src": "package.json",
            "use": "vercel-sapper"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.js"
        }
    ]
}